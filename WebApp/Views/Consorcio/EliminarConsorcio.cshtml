@using Entidades
@model Consorcio
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EliminarConsorcio</title>
</head>
<body>
    <div> 
        <p>Esta seguro que desea eliminar el consorcio @Model.Nombre ?</p>
        <form action="/consorcio/eliminarConsorcio" method="post">

            <input hidden value="@Model.IdConsorcio" />
            <button type="submit">Eliminar</button><br />
            <button type="submit" formaction="/consorcio/listarConsorcio" formmethod="get">Cancelar</button>

        </form>
    </div>
</body>
</html>
